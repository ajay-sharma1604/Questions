//Sum of digits of a number ,936=18

/*import java.util.Scanner;
public class Basics{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter input");
        int n = sc.nextInt();
        int sum =0;
        int value = 0;
        while(n>0) //(n!=0) 
        {
            value =n%10;
            n=n/10;
            sum=sum+value;
        }
        System.out.println(sum);
    }
}*/


//Prime Number

/*import java.util.*;
public class Basics{
    public static void main(String[] args) {
        Scanner sc =new Scanner(System.in);
        System.out.println("please enter input number");
        int input = sc.nextInt();
        if(input==2){
            System.out.println("Number is: Prime");
        }
        else{
            for(int i=2; i<=input-1; i++){
                if(input%i==0){
                    System.out.println("Number is: Not Prime");
                    return;
                }
            }
            System.out.println("Number is: Prime");
        }
    }
}*/

//Reverse Number

/*import java.util.*;
public class Basics{
    public static void main(String[] args) {
        Scanner sc=new Scanner (System.in);
        System.out.println("Please enter input");
        int input = sc.nextInt();
        int reverse = 0;
        while(input>0){
            int lastdigit = input%10;
            input = input/10;
            reverse =(reverse*10) + lastdigit;
        }
        System.out.println("Reverse is:" + reverse);
    }
}*/

//Palindrome Number- Number and its reverse are equal

/*import java.util.*;
public class Basics{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int rev =0;
        int newl =n;
    
        while(n>0){
            int lastdigit = n%10;
            rev = (rev*10)  + lastdigit;
            n=n/10;
        }

        if(newl==rev){
            System.out.println("Palindrome");
        }
        else
        System.out.println("Not a palindrome");
    }
}*/

//Armstrong Number- Sum of cube of all digits will be equal to original number

/*import java.util.Scanner;

public class Basics
{
    public static void main(String []args)
    {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int temp= num;
        double sum=0;

        for(; num>0; num=num/10)
        {
            int lastdigit= num%10;
            double num2 = Math.pow(lastdigit ,3);
            sum= sum+num2;
        }
        if(sum==temp)
        {
            System.out.println("Number is Armstrong");
        }
        else
        {
            System.out.println("Number is  not Armstrong");
        }
    }
}*/


//Perfect number- A number whose sum of factors(excluding number itself)
/*
import java.util.Scanner;
public class Basics{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int input = sc.nextInt();
        int sum =0;
        for(int i=1; i<input; i++){
            if(input%i==0){
                sum =sum +i;
               
            }
      
        }
        if(input==sum){
            System.out.println("Number is perfect");
        }
        else{
            System.out.println("Number is not perfect");
        }
    }
}
*/

//Strong number - Sum of factorial of each digit is equal to original number//

/*import java.util.Scanner;
public class Basics{
    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        System.out.println("Please enter input");
        int n = sc.nextInt();
        int fake = n;
        int sum =0;
       
        while(n>0){
            int lastdigit = n%10;
            int factorial = 1;
            n = n/10;
            for(int i =lastdigit; i>=1; i--){
                factorial = (factorial* i) ;
            }
            sum = sum + factorial;
        }

        if(sum == fake){
            System.out.println("Number is Strong");
        }
        else{
            System.out.println("Not Strong");
        }
    }
}*/


//Harshad number -Number completely divisible by sum of its digits.

/*import java.util.Scanner;
public class Basics{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter input");
        int n= sc.nextInt();
        int fake =n;
        int sum = 0;
        while(n>0){
            int lastdigit = n%10;
            n=n/10;
            sum = sum + lastdigit;
        }
        if(fake%sum==0){
            System.out.println("Harshad number");
        }
        else{
            System.out.println("False");
        }
    }
}*/

//fibonacci series

/*import java.util.Scanner;
public class Basics{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n1=0;
        int n2 =1;
        System.out.print(n1 + " " + n2 + " ");
        for(int i=3;i<=50;i++){
            int n =(n1 + n2);
            System.out.print(n + " ");
            n1=n2;
            n2=n;
        }
    }
}*/



//HCF HIGHEST COMMON FACTOR -Biggest Common Factor

/*import java.util.Scanner;
public class Basics{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("enter small number");
        int n1=sc.nextInt();
        System.out.println("enter big number");
        int n2=sc.nextInt();
        int result = 0;

        for(int i=1 ; i<=n1; i++){
            if(n1%i==0 && n2%i == 0){
                result = i;
            }
        }
        System.out.println(result);
    }
}*/




